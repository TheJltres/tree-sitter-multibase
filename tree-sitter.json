{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "multibase",
      "camelcase": "MultiBase",
      "title": "MultiBase",
      "scope": "source.multibase",
      "file-types": [
        "mb"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^multibase$",
      "class-name": "TreeSitterMultibase"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the base100 language MultiBase",
    "authors": [
      {
        "name": "Jose Luis Tresserras Merino",
        "email": "joseluistresserras@antex.net"
      }
    ],
    "links": {
      "repository": "https://github.com/thejltres/tree-sitter-multibase"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}
